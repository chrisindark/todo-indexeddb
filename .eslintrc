{
  "extends": [ "eslint:recommended" ],
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-var": 2,
    "no-console": 0,
    "comma-dangle": 0,
    "require-jsdoc": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
